    "./package.json": "./package.json"
  },
  "main": "./src/index.js",
  "types": "./types/index.d.ts",
  "repository": "regseb/playwright-ghost",
  "type": "module",
  "scripts": {
    "prepare": "tsc --project .tsconfig_types.json",
    "lint": "metalint",
    "lint:fix": "metalint --fix",
    "lint:types": "tsc --project .tsconfig_lint.json",
    "test": "npm run test:coverage",
    "test:unit": "node --test 'test/unit/**/*.js'",
    "test:integration": "node --test 'test/integration/**/*.js'",
    "test:coverage": "stryker run",
    "jsdocs": "typedoc --tsconfig .tsconfig_jsdocs.json",
    "clean": "node .script/clean.js"
  },
  "dependencies": {
    "@ghostery/adblocker-playwright": ">=2.11.3",
    "fingerprint-generator": ">=2.1.69",
    "fingerprint-injector": ">=2.1.69",
    "ghost-cursor": "1.4.1"
  },
  "devDependencies": {
    "@prantlf/jsonlint": "16.0.0",
    "@prettier/plugin-xml": "3.4.1",
    "@stryker-mutator/core": "8.7.1",
    "@stryker-mutator/tap-runner": "8.7.1",
    "@types/eslint-plugin-mocha": "10.4.0",
    "@types/node": "22.15.2",
    "eslint": "9.25.1",
    "eslint-plugin-array-func": "5.0.2",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsdoc": "50.6.11",
    "eslint-plugin-mocha": "11.0.0",
    "eslint-plugin-n": "17.17.0",
    "eslint-plugin-no-unsanitized": "4.1.2",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-regexp": "2.7.0",
    "eslint-plugin-unicorn": "59.0.0",
    "globals": "16.0.0",
    "markdownlint": "0.37.4",
    "metalint": "0.20.0",
    "npm-package-json-lint": "8.0.0",
    "patchright": "1.52.0",
    "playwright": "1.52.0",
    "prettier": "3.5.3",
    "publint": "0.3.12",
    "puppeteer": "24.14.0",
    "rebrowser-playwright": "1.49.1",
    "typedoc": "0.28.3",
    "typescript": "5.8.3",
    "yaml-lint": "1.7.0"
  },
  "peerDependencies": {
    "patchright": ">=1.50.0",
    "playwright": ">=1.50.0",
    "rebrowser-playwright": ">=1.49.0"
  },
  "peerDependenciesMeta": {
    "patchright": {
      "optional": true
    },
    "playwright": {
      "optional": true
    },
    "rebrowser-playwright": {
      "optional": true
    }
  },
